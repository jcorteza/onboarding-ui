{
  "name": "onboarding-ui",
  "version": "1.0.0",
  "description": "This simple web page runs on its own server created using Node. [Download Node](https://nodejs.org/en/) if you don't already have it. I recommend the LTS version, which is more stable and widely supported.",
  "scripts": {
    "hello": "echo \"Hello World!\"",
    "test": "jest",
    "watch": "webpack --watch",
    "update-styles": "sass ./src/style/sass/main.scss ./src/style/css/main.css",
    "dev": "npm run update-styles && npm run build && webpack-dev-server --config './webpack.config.js'",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcorteza/onboarding-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcorteza/onboarding-ui/issues"
  },
  "homepage": "https://github.com/jcorteza/onboarding-ui#readme",
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/_test_/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/_test_/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": ["<rootDir>/src/__test__/enzyme.config.js"],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.jsx$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "jest": "^24.8.0",
    "sass": "^1.21.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
